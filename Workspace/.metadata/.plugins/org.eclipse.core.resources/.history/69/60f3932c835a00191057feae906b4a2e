package ch04;
//1 ~ 1000 사이의 숫자 중 3을 포함하고 있는 숫자를 출력
public class Exam4_8 {

	public static void main(String[] args) {
		int n1;
		int n2;
		int n3;
		//999
		for (int i = 1; i <= 1000; i++) {
			n3 = ((i / 10)/ 10);
			n2 = (i / 10) % 10;			
			n1 = i % 10;				//1의자리
					//10의 자리	
						//100의 자리
				
				if (n1 ==3 || n2 ==3 || n3 ==3){
				System.out.print(i);
				System.out.println(": 3 포함");
				}
			
			//
			}
		} // 코드 작성 }
	}
