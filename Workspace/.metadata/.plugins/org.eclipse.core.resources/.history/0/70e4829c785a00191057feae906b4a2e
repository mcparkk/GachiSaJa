package ch04;
//1 ~ 1000 사이의 숫자 중 3을 포함하고 있는 숫자를 출력
public class Exam4_8 {

	public static void main(String[] args) {
		int n1;
		int n2;
		int n3;
		
		for (int i = 1; i <= 1000; i++) {
			for (int j = 0; j < 3; j++) {
				n1 = i % 10;
				n2 = i % 10 / 10;
				n3 = i % 10/ 10/ 10;
				System.out.println(+n1);
			}//
			}
		} // 코드 작성 }
	}
}